      "Code": "input = IN[0]\r\nif input != 0:\r\n\timport sys\r\n\timport clr\r\n\t\r\n\tfrom System import Array\r\n\tfrom System.Collections.Generic import *\r\n\t\r\n\tfrom operator import attrgetter\r\n\t\r\n\tclr.AddReference('ProtoGeometry')\r\n\tfrom Autodesk.DesignScript.Geometry import *\r\n\t\r\n\tclr.AddReference('RevitAPI')\r\n\timport Autodesk\r\n\tfrom Autodesk.Revit.DB import *\r\n\t\r\n\tclr.AddReference(\"RevitAPIUI\")\r\n\timport Autodesk \r\n\tfrom Autodesk.Revit.UI import TaskDialog, IExternalEventHandler, ExternalEvent\r\n\tfrom Autodesk.Revit.UI.Selection import *\r\n\t\r\n\tclr.AddReference(\"RevitServices\")\r\n\timport RevitServices\r\n\tfrom RevitServices.Persistence import DocumentManager\r\n\tfrom RevitServices.Transactions import TransactionManager\r\n\t\r\n\tclr.AddReference('RevitAPIIFC')\r\n\tfrom Autodesk.Revit.DB.IFC import *\r\n\t\r\n\tdef unitConverterToInternal(value, units):\r\n\t\tif value != 0:\r\n\t\t\t_inchToMm = float(25.4)\r\n\t\t\t_footToMm = float(12) * _inchToMm\r\n\t\t\tif units ==\"in MM\":\r\n\t\t\t\treturn float(value)/_footToMm\r\n\t\t\telif units ==\"in M\":\r\n\t\t\t\treturn float(value)*1000/_footToMm\r\n\t\t\telif units ==\"in FT\":\r\n\t\t\t\treturn float(value)\r\n\t\t\telif units ==\"in INCH\":\r\n\t\t\t\treturn (float(value)*_inchToMm)/_footToMm\r\n\t\telse:\r\n\t\t\treturn 0\r\n\t\r\n\tclass RoomSelectionFilter(ISelectionFilter):        \r\n\t\tdef AllowElement(self, element):\r\n\t\t\tif element.Category.Id.IntegerValue==int(BuiltInCategory.OST_Rooms):\r\n\t\t\t\treturn True\r\n\t\t\telse:\r\n\t\t\t\treturn False\r\n\t\r\n\t\tdef AllowReference(self, refer, point):\r\n\t\t\treturn False\r\n\t\r\n\tdef pickobject():\r\n\t\troomFilter = RoomSelectionFilter()\r\n\t\tpicked = uidoc.Selection.PickObjects(ObjectType.Element, roomFilter)\r\n\t\treturn picked\r\n\t\r\n\tdef createFloors(doc, all_rooms, floorType, RVTversion, inputs):\r\n\t\tfor room in all_rooms:\r\n\t\t\troomLevel = doc.GetElement(room.LevelId)\r\n\t\t\t#Get boundaries\r\n\t\t\topt = SpatialElementBoundaryOptions()\r\n\t\t\t#opt.SpatialElementBoundaryLocation = SpatialElementBoundaryLocation.Finish\r\n\t\t\tboundaries = room.GetBoundarySegments(opt)\r\n\t\t\tcurveArray = CurveArray()\r\n\t\t\tcurveLoops = List[CurveLoop]()\r\n\t\t\tif len(boundaries) > 0:\r\n\t\t\t\t#Different loops for different RVT versions\r\n\t\t\t\tfor bnd in boundaries[0]:\r\n\t\t\t\t\tcurveArray.Append(bnd.GetCurve())\r\n\t\t\t\tfor bnd in boundaries:\r\n\t\t\t\t\tcurveLoop = CurveLoop()\r\n\t\t\t\t\tfor seg in bnd:\r\n\t\t\t\t\t\tcurveLoop.Append(seg.GetCurve())\r\n\t\t\t\t\tcurveLoops.Add(curveLoop)\r\n\t\t\t\t#Get height parameter from window \r\n\t\t\t\t#Create Floor\r\n\t\t\t\tif curveArray.Size !=0:\r\n\t\t\t\t\tif RVTversion > 2021:\r\n\t\t\t\t\t\tfloor = Floor.Create(doc, curveLoops, floorType.Id, roomLevel.Id)\r\n\t\t\t\t\telse:\r\n\t\t\t\t\t\tfloor = doc.Create.NewFloor(curveArray, floorType, roomLevel, False)\r\n\t\t\t\t\t#Get height above level(if any)\r\n\t\t\t\t\theight=0\r\n\t\t\t\t\tif inputs[2]!=None:\r\n\t\t\t\t\t\theight = unitConverterToInternal(inputs[2], inputs[3])\r\n\t\t\t\t\telse:\r\n\t\t\t\t\t\t#Get value from parameter\r\n\t\t\t\t\t\troomParameter = room.get_Parameter(inputs[4])\r\n\t\t\t\t\t\theight = roomParameter.AsDouble();\r\n\t\t\t\t\tparam = floor.get_Parameter(BuiltInParameter.FLOOR_HEIGHTABOVELEVEL_PARAM)\r\n\t\t\t\t\tparam.Set(height);\r\n\t\treturn 0\r\n\t\r\n\tdoc = DocumentManager.Instance.CurrentDBDocument\r\n\tuidoc = DocumentManager.Instance.CurrentUIApplication.ActiveUIDocument\r\n\tapp = doc.Application\r\n\tversion = int(app.VersionNumber)\r\n\t#View has to be Floorplan, CeilingPlan, AreaPlan or EngPlan\t\r\n\tinpt_all_rooms = str(input[0])\r\n\tinpt_sel_type = doc.GetElement(input[1])\r\n\tif inpt_all_rooms == \"True\":\r\n\t\tsel_View = doc.ActiveView\r\n\t\t#Project or System browser selected\r\n\t\tif sel_View.ViewType == ViewType.ProjectBrowser or sel_View.ViewType == ViewType.SystemBrowser:\r\n\t\t\tsel_View = doc.GetElement(uidoc.GetOpenUIViews()[0].ViewId)\r\n\t\tif sel_View.ViewType == ViewType.FloorPlan or sel_View.ViewType == ViewType.CeilingPlan or sel_View.ViewType == ViewType.AreaPlan or sel_View.ViewType == ViewType.EngineeringPlan:\r\n\t\t\t#Get all rooms of current view\r\n\t\t\tall_rooms = [room for room in FilteredElementCollector(doc, sel_View.Id).OfClass(SpatialElement) if isinstance(room, Architecture.Room)]\r\n\t\t\t#TransactionManager.Instance.EnsureInTransaction(doc)\r\n\t\t\twith Transaction(doc, \"Create Floor Finish by Room/s\") as trans:\r\n\t\t\t\ttrans.Start()\r\n\t\t\t\tall_floors = createFloors(doc, all_rooms, inpt_sel_type, version, input)\r\n\t\t\t\ttrans.Commit()\r\n\t\t\t#TransactionManager.Instance.TransactionTaskDone()\r\n\telse:\r\n\t\t#Get selected rooms if false\r\n\t\tinpt_sel_rooms = List[ElementId]()\r\n\t\tall_rooms = List[Element]()\r\n\t\tif uidoc.Selection.GetElementIds().Count == 0:\r\n\t\t\t#Nothing selected, let the user select\r\n\t\t\ttry:\r\n\t\t\t\tinpt_sel_rooms = pickobject()\r\n\t\t\texcept:\r\n\t\t\t\tpass\r\n\t\telse:\r\n\t\t\tinpt_sel_rooms = uidoc.Selection.GetElementIds()\r\n\t\tfor item in inpt_sel_rooms:\r\n\t\t\telemento = doc.GetElement(item)\r\n\t\t\tif isinstance(elemento, Architecture.Room):\r\n\t\t\t\tall_rooms.Add(elemento)\r\n\t\tif len(all_rooms) == 0:\t\t\r\n\t\t\ttask2 = Autodesk.Revit.UI.TaskDialog(\"Create Floor Finish by Room/s\")\r\n\t\t\ttask2.MainInstruction = \"No rooms were selected in Revit.\"\r\n\t\t\ttask2.Show()\r\n\t\t\tuidoc.Selection.SetElementIds(List[ElementId]())\r\n\t\telse:\r\n\t\t\t#TransactionManager.Instance.EnsureInTransaction(doc)\r\n\t\t\twith Transaction(doc, \"Create Floor Finish by Room/s\") as trans:\r\n\t\t\t\ttrans.Start()\r\n\t\t\t\tall_floors = createFloors(doc, all_rooms, inpt_sel_type, version, input)\r\n\t\t\t\ttrans.Commit()\r\n\t\t\t#TransactionManager.Instance.TransactionTaskDone()\r\nOUT=0\r\n\"\"\"\r\nThis script is based on Room 